<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="medical_beneficts_base_tree" model="ir.ui.view">
            <field name="name">medical.beneficts.tree</field>
            <field name="model">medical.beneficts</field>
            <field name="arch" type="xml">
                <tree string="Medical Beneficts" editable="bottom">
                    <field name="partner_id" />
                    <field name="relationship_id" />
                    <field name="inclusion_date" />
                </tree>
            </field>
        </record>
        <record id="medical_beneficts_action" model="ir.actions.act_window">
            <field name="name">Medical Beneficts</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">medical.beneficts</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Create New Medical Beneficts</p>
            </field>
        </record>
    </data>
</odoo>
